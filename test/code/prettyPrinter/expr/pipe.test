Pipe operator
-----
<?php
$a |> $b |> $c;
$a . $b |> $c . $d;
$a |> $b == $c;
$c == $a |> $b;
($a == $b) |> ($c == $d);
$a . ($b |> $c) . $d;
$a |> (fn($x) => $x) |> (fn($y) => $y) |> $b;
(fn($x) => $x) |> $y;
fn($x) => $x |> $y;
